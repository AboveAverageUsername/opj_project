Dialogue: 0,0:00:14.91,0:00:21.58,datum,,0000,0000,0000,,11 NOVEMBAR 2012
Dialogue: 0,0:00:14.91,0:00:21.58,overdose,,0000,0000,0000,,Ovo izdanje nije za prodaju ili iznajmljivanje!\NOVO JE BESPLATAN FANSUB!\Nhttp://www.anime-overdose.com
Dialogue: 0,0:01:30.62,0:01:34.37,Naslov,Default,0000,0000,0000,,{\i1\pos(594,396)\fscx126.25\fscy236.25}JEDAN VREDAN KLAĐENJA{\i0}
Dialogue: 0,0:01:43.52,0:01:48.00,Default00,Default,0000,0000,0000,,Kao što sam objasnio na putu,\Nnaša misija je bila
Dialogue: 0,0:01:48.04,0:01:51.07,Default00,Default,0000,0000,0000,,da nabavimo Tajnu Veštinu iz Kedar sela.
Dialogue: 0,0:01:51.36,0:01:55.06,Default00,Default,0000,0000,0000,,Sakupljanje Tajnih Veština…\NIma li tu nešto novo?
Dialogue: 0,0:01:56.45,0:01:57.74,Default00,Default,0000,0000,0000,,Nastavi…
Dialogue: 0,0:01:57.90,0:02:02.83,Default00,Default,0000,0000,0000,,Nakon što smo nabavili Tajnu Veštinu,\Notišao sam da zbrinem drugu stvar, zato…
Dialogue: 0,0:02:02.99,0:02:05.03,Default00,Default,0000,0000,0000,,Ja ću sada nastaviti.
Dialogue: 0,0:02:11.79,0:02:13.24,Default00,Default,0000,0000,0000,,{\i1}Gospodaru Raikage…{\i0}
Dialogue: 0,0:02:14.87,0:02:16.28,Default00,Default,0000,0000,0000,,{\i1}Ostavite…{\i0}
Dialogue: 0,0:02:17.49,0:02:20.08,Default00,Default,0000,0000,0000,,{\i1}Ostavite…{\i0}
Dialogue: 0,0:02:24.87,0:02:28.24,Default00,Default,0000,0000,0000,,{\i1}Šta? Dimnu zavesu?{\i0}
Dialogue: 0,0:02:28.24,0:02:30.19,Default00,Default,0000,0000,0000,,{\i1}Smatraš nas budalama!{\i0}
Dialogue: 0,0:02:30.24,0:02:32.95,Default00,Default,0000,0000,0000,,{\i1}Ostavi to…{\i0}
Dialogue: 0,0:02:33.53,0:02:34.78,Default00,Default,0000,0000,0000,,{\i1}Ko si ti?{\i0}
Dialogue: 0,0:02:34.99,0:02:36.69,Default00,Default,0000,0000,0000,,{\i1}Da li si ti iz Kedar sela?{\i0}
Dialogue: 0,0:02:39.20,0:02:42.33,Default00,Default,0000,0000,0000,,{\i1}Mi nismo nindže iz Kedar sela!{\i0}
Dialogue: 0,0:02:42.45,0:02:46.24,Default00,Default,0000,0000,0000,,{\i1}Mi nemamo domovinu, niti gospodara.{\i0}
Dialogue: 0,0:02:46.24,0:02:50.41,Default00,Default,0000,0000,0000,,{\i1}Ljudi nas potraže,{\i0}\N{\i1}i mi ispunjavamo njihove želje.{\i0}
Dialogue: 0,0:02:51.08,0:02:55.41,Default00,Default,0000,0000,0000,,{\i1}Kedar selo nam je prišlo i{\i0}\N{\i1}njihova želja je…{\i0}
Dialogue: 0,0:02:55.51,0:02:59.49,Default00,Default,0000,0000,0000,,{\i1}Smrt onima koji su ukrali{\i0}\N{\i1}Tajnu Veštinu Kedar sela!{\i0}
Dialogue: 0,0:02:59.74,0:03:01.74,Default00,Default,0000,0000,0000,,{\i1}Nindže Plaćenici, eh!{\i0}
Dialogue: 0,0:03:02.03,0:03:03.71,Default00,Default,0000,0000,0000,,{\i1}Napad Munjom!{\i0}
Dialogue: 0,0:03:11.33,0:03:12.28,Default00,Default,0000,0000,0000,,{\i1}Karai!{\i0}
Dialogue: 0,0:03:12.28,0:03:14.06,Default00,Default,0000,0000,0000,,{\i1}Karai, šta nije u redu?!{\i0}
Dialogue: 0,0:03:16.28,0:03:17.70,Default00,Default,0000,0000,0000,,{\i1}Zašto ti...!{\i0}
Dialogue: 0,0:03:19.08,0:03:22.53,Default00,Default,0000,0000,0000,,{\i1}Mi smo lutajući fantomi iz šume.{\i0}
Dialogue: 0,0:03:26.37,0:03:27.58,Default00,Default,0000,0000,0000,,{\i1}Nokizaru!{\i0}
Dialogue: 0,0:03:28.12,0:03:29.37,Default00,Default,0000,0000,0000,,{\i1}Nokizaru…?{\i0}
Dialogue: 0,0:03:32.53,0:03:35.37,Default00,Default,0000,0000,0000,,{\i1}Nindže koji love druge nindže…{\i0}
Dialogue: 0,0:03:35.37,0:03:37.12,Default00,Default,0000,0000,0000,,{\i1}Nokizaru Trupe, eh?{\i0}
Dialogue: 0,0:03:41.77,0:03:43.22,Default00,Default,0000,0000,0000,,{\i1}Gospodaru Raikage!{\i0}
Dialogue: 0,0:03:44.37,0:03:46.24,Default00,Default,0000,0000,0000,,{\i1}Karai nije u stanju da…{\i0}
Dialogue: 0,0:03:46.28,0:03:48.17,Default00,Default,0000,0000,0000,,{\i1}Dobro sam. Idemo!{\i0}
Dialogue: 0,0:03:50.70,0:03:53.69,Default00,Default,0000,0000,0000,,{\i1}To je sve? Oni su prave kukavice!{\i0}
Dialogue: 0,0:03:54.28,0:03:59.36,Default00,Default,0000,0000,0000,,{\i1}Gospodaru Raikage, nemojte dozvoliti{\i0}\N{\i1}da vas neprijatelj povocira!{\i0}
Dialogue: 0,0:03:59.40,0:04:03.02,Default00,Default,0000,0000,0000,,{\i1}Mora da postoji ispred nas{\i0}\N{\i1}neko mesto gde možemo da odemo.{\i0}
Dialogue: 0,0:04:03.04,0:04:06.80,Default00,Default,0000,0000,0000,,{\i1}Raikage iz sela u Oblacima{\i0}\N{\i1}nije baš ništa posebno…{\i0}
Dialogue: 0,0:04:09.33,0:04:11.12,Default00,Default,0000,0000,0000,,{\i1}Šta...?!{\i0}
Dialogue: 0,0:04:11.36,0:04:13.51,Default00,Default,0000,0000,0000,,{\i1}Dođavola! Ne podcenjujte nas!{\i0}
Dialogue: 0,0:04:14.74,0:04:16.87,Default00,Default,0000,0000,0000,,{\i1}Munjeviti Snopovi!{\i0}
Dialogue: 0,0:04:20.83,0:04:22.33,Default00,Default,0000,0000,0000,,{\i1}Nigai!{\i0}
Dialogue: 0,0:04:23.08,0:04:24.28,Default00,Default,0000,0000,0000,,{\i1}Ne prilazite!{\i0}
Dialogue: 0,0:04:24.49,0:04:25.83,Default00,Default,0000,0000,0000,,{\i1}Molim…{\i0}
Dialogue: 0,0:04:25.87,0:04:27.70,Default00,Default,0000,0000,0000,,{\i1}ne prilazite…{\i1}{\i0}{\i0}
Dialogue: 0,0:04:31.33,0:04:33.16,Default00,Default,0000,0000,0000,,{\i1}Nigai!{\i0}
Dialogue: 0,0:04:33.94,0:04:35.37,Default00,Default,0000,0000,0000,,{\i1}Zašto…!{\i1}{\i0}{\i0}
Dialogue: 0,0:04:36.03,0:04:38.20,Default00,Default,0000,0000,0000,,{\i1}Nećete se izvući s ovim!{\i1}{\i0}{\i0}
Dialogue: 0,0:04:41.26,0:04:42.97,Default00,Default,0000,0000,0000,,{\i1}Ne, Gospodaru Raikage!{\i1}{\i0}{\i0}
Dialogue: 0,0:04:42.97,0:04:45.37,Default00,Default,0000,0000,0000,,{\i1}Oboje, Nigai i ja smo koristili naše veštine{\i1}{\i0}{\i0}
Dialogue: 0,0:04:45.41,0:04:47.41,Default00,Default,0000,0000,0000,,{\i1}i to je izazvalo anomaliju{\i0}\N{\i1}u našim telima…{\i1}{\i0}{\i0}
Dialogue: 0,0:04:47.45,0:04:49.36,Default00,Default,0000,0000,0000,,{\i1}Ako ti isto budeš koristio svoju veštinu…{\i1}{\i0}{\i0}
Dialogue: 0,0:04:49.38,0:04:50.89,Default00,Default,0000,0000,0000,,{\i1}Ostani pozadi, Karai!{\i1}{\i0}{\i0}
Dialogue: 0,0:04:57.84,0:05:02.03,Default00,Default,0000,0000,0000,,Možda su videli da sam veoma ozbiljan\Npa su se uplašili i otišli…
Dialogue: 0,0:05:02.08,0:05:05.41,Default00,Default,0000,0000,0000,,Nisu se ponovo pojavljivali.
Dialogue: 0,0:05:06.58,0:05:08.83,Default00,Default,0000,0000,0000,,Ali pitam se zašto?
Dialogue: 0,0:05:09.24,0:05:11.28,Default00,Default,0000,0000,0000,,Oni su bili napadači…
Dialogue: 0,0:05:12.18,0:05:16.28,Default00,Default,0000,0000,0000,,Sem toga, Nigai je koristio svoju veštinu\Ni to je izazvalo da on eksplodira.
Dialogue: 0,0:05:16.41,0:05:18.58,Default00,Default,0000,0000,0000,,Karai je isto koristio svoju veštinu.
Dialogue: 0,0:05:18.62,0:05:20.70,Default00,Default,0000,0000,0000,,Shvatio je promenu u svom telu,
Dialogue: 0,0:05:20.75,0:05:24.30,Default00,Default,0000,0000,0000,,i brzo potisnuo sve daljne aktivnosti\Nda bi sprečio eksploziju.
Dialogue: 0,0:05:24.33,0:05:27.95,Default00,Default,0000,0000,0000,,Ali čakra sa osobinama Papirne Bombe\Nkoja je zakačena u njegovim grudima
Dialogue: 0,0:05:27.99,0:05:30.77,Default00,Default,0000,0000,0000,,i dalje upija njegovu čakru,\Ni on je u velikim bolovima.
Dialogue: 0,0:05:30.99,0:05:35.08,Default00,Default,0000,0000,0000,,Gospodar Raikage manipuliše\Nnajsnažniju od svih čakri.
Dialogue: 0,0:05:35.33,0:05:37.37,Default00,Default,0000,0000,0000,,Ali on je živ i zdrav.
Dialogue: 0,0:05:38.60,0:05:40.33,Default00,Default,0000,0000,0000,,On nije uopšte zdrav.
Dialogue: 0,0:05:41.87,0:05:44.28,Default00,Default,0000,0000,0000,,Dok ga gledaš sada, ne može se reći.
Dialogue: 0,0:05:44.33,0:05:46.41,Default00,Default,0000,0000,0000,,Ali ja mogu da osetim u Raikageu,
Dialogue: 0,0:05:46.45,0:05:48.41,Default00,Default,0000,0000,0000,,istu vrstu čakre kao u tom otoku.
Dialogue: 0,0:05:48.58,0:05:51.53,Default00,Default,0000,0000,0000,,Kako to misliš?\NOsećam se savršeno dobro!
Dialogue: 0,0:05:52.89,0:05:54.33,Default00,Default,0000,0000,0000,,Za sada, ti jesi.
Dialogue: 0,0:05:54.86,0:05:58.45,Default00,Default,0000,0000,0000,,Sigurana sam da je ovaj otok\NEksplodirajuća Buba
Dialogue: 0,0:05:58.49,0:06:02.34,Default00,Default,0000,0000,0000,,koju su samo Nokizaru Trupe\Nuspele uspešno da razviju.
Dialogue: 0,0:06:02.95,0:06:04.78,Default00,Default,0000,0000,0000,,Video si šta ono radi.
Dialogue: 0,0:06:04.83,0:06:08.37,Default00,Default,0000,0000,0000,,Jednom kad pronađe domaćina,\Nto upija čakru i raste,
Dialogue: 0,0:06:08.41,0:06:10.08,Default00,Default,0000,0000,0000,,i neizbežno eksplodira.
Dialogue: 0,0:06:10.83,0:06:13.66,Default00,Default,0000,0000,0000,,Veličina larve zahteva zabrinutost.
Dialogue: 0,0:06:14.12,0:06:14.91,Default00,Default,0000,0000,0000,,Huh?
Dialogue: 0,0:06:16.13,0:06:17.54,Default00,Default,0000,0000,0000,,Osetio si to?
Dialogue: 0,0:06:18.20,0:06:22.41,Default00,Default,0000,0000,0000,,Larve su sićušne kao male ćelije,\Nnevidljive za ljudsko oko.
Dialogue: 0,0:06:22.41,0:06:23.49,Default00,Default,0000,0000,0000,,Onda…
Dialogue: 0,0:06:23.73,0:06:24.74,Default00,Default,0000,0000,0000,,Da…
Dialogue: 0,0:06:24.76,0:06:28.99,Default00,Default,0000,0000,0000,,Upali ste u neprijateljsku zamku\Njoš pri prvom kontaktu.
Dialogue: 0,0:06:30.54,0:06:34.58,Default00,Default,0000,0000,0000,,Larve su raštrkane u atmosferu\Nsa onom eksplozijom svetlosti…
Dialogue: 0,0:06:34.63,0:06:38.15,Default00,Default,0000,0000,0000,,i vi ste nastavili da ih udišete,\Nnesvesni o bilo čemu.
Dialogue: 0,0:06:39.33,0:06:41.66,Default00,Default,0000,0000,0000,,Onda zašto je Gospodar Raikage…?
Dialogue: 0,0:06:41.83,0:06:43.20,Default00,Default,0000,0000,0000,,Ona je u pravu!
Dialogue: 0,0:06:43.53,0:06:46.24,Default00,Default,0000,0000,0000,,Zašto ja nemam otok?
Dialogue: 0,0:06:46.37,0:06:49.16,Default00,Default,0000,0000,0000,,Bio sam u istoj situaciji \Nkao Karai i Nigai!
Dialogue: 0,0:06:49.91,0:06:54.83,Default00,Default,0000,0000,0000,,I ne mogu reći da nisam koristio čakru!
Dialogue: 0,0:06:54.90,0:06:59.87,Default00,Default,0000,0000,0000,,Ranije, dok smo obarali ruke,\Nkoristio sam prilično mnogo čakre.
Dialogue: 0,0:07:00.13,0:07:04.62,Default00,Default,0000,0000,0000,,Nestašno derište iz Oblaka\Nsamo je poraslo u veličini.
Dialogue: 0,0:07:04.65,0:07:06.24,Default00,Default,0000,0000,0000,,Kako se usuđuješ…!
Dialogue: 0,0:07:06.28,0:07:07.70,Default00,Default,0000,0000,0000,,Hajde, hajde…
Dialogue: 0,0:07:07.84,0:07:11.24,Default00,Default,0000,0000,0000,,Saslušaj me pre nego što \Npočneš da pričaš.
Dialogue: 0,0:07:13.22,0:07:14.82,Default00,Default,0000,0000,0000,,Na prvom mestu,
Dialogue: 0,0:07:14.84,0:07:18.40,Default00,Default,0000,0000,0000,,mi nindže aktiviramo naše veštine\Nmanipulišući čakru.
Dialogue: 0,0:07:18.68,0:07:21.28,Default00,Default,0000,0000,0000,,Količina čakre zavisi od nivoa veštine.
Dialogue: 0,0:07:21.48,0:07:26.24,Default00,Default,0000,0000,0000,,I mi podešavamo količinu sa svakom\Nveštinom koju koristimo.
Dialogue: 0,0:07:27.83,0:07:32.87,Default00,Default,0000,0000,0000,,Što je snažnija veština,\Nviše čakre zahteva.
Dialogue: 0,0:07:33.43,0:07:38.58,Default00,Default,0000,0000,0000,,Zbog toga, mi skladištimo čakru\Nu našem telu na određeno vreme.
Dialogue: 0,0:07:39.03,0:07:40.78,Default00,Default,0000,0000,0000,,Samo trenutak…
Dialogue: 0,0:07:40.85,0:07:44.78,Default00,Default,0000,0000,0000,,Ranije, pomenuli ste meč u \Nobaranju ruke između vas dvoje.
Dialogue: 0,0:07:44.91,0:07:48.70,Default00,Default,0000,0000,0000,,Zar ti ne koristiš čakru kada\Nkoristiš fizičku snagu?
Dialogue: 0,0:07:49.24,0:07:53.74,Default00,Default,0000,0000,0000,,Da... Međutim, Raikage nije koristio\Nčakru za snagu.
Dialogue: 0,0:07:55.37,0:07:58.41,Default00,Default,0000,0000,0000,,Da kontrira mojoj fizičkoj snazi…
Dialogue: 0,0:07:58.48,0:08:00.70,Default00,Default,0000,0000,0000,,on je koristio brzinu da bi me odgurao.
Dialogue: 0,0:08:05.51,0:08:08.20,Default00,Default,0000,0000,0000,,Prirodno, za brzinu je isto \Npotrebna čakra.
Dialogue: 0,0:08:08.24,0:08:12.74,Default00,Default,0000,0000,0000,,Pretvoriti čakru u brzinu zahteva\Ntrenutnu potrošnju…
Dialogue: 0,0:08:12.79,0:08:16.50,Default00,Default,0000,0000,0000,,Nedovoljno vremena za \NEksplodirajuću Bombu da ukrade čakru.
Dialogue: 0,0:08:17.20,0:08:20.53,Default00,Default,0000,0000,0000,,Dakle, ja ću završiti kao Karai?
Dialogue: 0,0:08:20.87,0:08:22.16,Default00,Default,0000,0000,0000,,Tačno!
Dialogue: 0,0:08:22.55,0:08:26.41,Default00,Default,0000,0000,0000,,Svi koristimo pomalo čakre u \Ndnevnim aktivnostima.
Dialogue: 0,0:08:26.48,0:08:29.99,Default00,Default,0000,0000,0000,,Zato, dok insekt ne bude uklonjen,\Nti moraš ostati smiren.
Dialogue: 0,0:08:30.83,0:08:33.24,Default00,Default,0000,0000,0000,,Onda to može da se ukloni?
Dialogue: 0,0:08:33.31,0:08:37.20,Default00,Default,0000,0000,0000,,Da. Jednostavna operacija \Nbi trebala da je ukloni.
Dialogue: 0,0:08:37.78,0:08:39.95,Default00,Default,0000,0000,0000,,Shizune, spremi se za operaciju!
Dialogue: 0,0:08:40.20,0:08:41.12,Default00,Default,0000,0000,0000,,Da!
Dialogue: 0,0:08:41.16,0:08:41.78,Default00,Default,0000,0000,0000,,I…
Dialogue: 0,0:08:41.83,0:08:44.45,Default00,Default,0000,0000,0000,,Oh, molim te, sačekaj! Ja ću asistirati!
Dialogue: 0,0:08:44.53,0:08:45.74,Default00,Default,0000,0000,0000,,Hvala.
Dialogue: 0,0:08:47.60,0:08:48.99,Default00,Default,0000,0000,0000,,Šta je bilo?
Dialogue: 0,0:08:49.18,0:08:52.43,Default00,Default,0000,0000,0000,,Huh? Oh, prvo ćemo operisati tvog podanika
Dialogue: 0,0:08:52.49,0:08:56.46,Default00,Default,0000,0000,0000,,zato što je on suočen s rizikom da \Neksplodira svakog trenutka.
Dialogue: 0,0:08:56.70,0:09:00.53,Default00,Default,0000,0000,0000,,To je u redu. Ja želim da spasite Karaia.
Dialogue: 0,0:09:00.81,0:09:02.70,Default00,Default,0000,0000,0000,,Ne, molim te sačekaj!
Dialogue: 0,0:09:02.73,0:09:05.49,Default00,Default,0000,0000,0000,,Morate pustiti da \NGospodar Raikage bude prvi!
Dialogue: 0,0:09:05.50,0:09:06.48,Default00,Default,0000,0000,0000,,Nemoguće!
Dialogue: 0,0:09:06.53,0:09:08.75,Default00,Default,0000,0000,0000,,Rekla sam da je sama operacija jednostavna.
Dialogue: 0,0:09:08.77,0:09:11.28,Default00,Default,0000,0000,0000,,Međutim, ako je Eksplodirajuća Buba\Npripojila sebe
Dialogue: 0,0:09:11.30,0:09:12.88,Default00,Default,0000,0000,0000,,komplikovano sa Čakra Sistemom,
Dialogue: 0,0:09:12.92,0:09:15.86,Default00,Default,0000,0000,0000,,to će zahtevati poprilično \Nmedicinsko iskustvo i veštinu.
Dialogue: 0,0:09:15.87,0:09:19.24,Default00,Default,0000,0000,0000,,Pored toga, to bi moglo \Neksplodirati bilo kada,
Dialogue: 0,0:09:19.58,0:09:22.91,Default00,Default,0000,0000,0000,,tako da hirurg isto rizikuje \Nda izgubi svoj život!
Dialogue: 0,0:09:23.58,0:09:26.28,Default00,Default,0000,0000,0000,,Zato ja neću da slušam tvoje gluposti!
Dialogue: 0,0:09:26.53,0:09:29.87,Default00,Default,0000,0000,0000,,U redu je, Tsunade.\NJa se slažem s tim.
Dialogue: 0,0:09:29.91,0:09:31.70,Default00,Default,0000,0000,0000,,Pobrini se prvo za Karaia.
Dialogue: 0,0:09:32.35,0:09:34.49,Default00,Default,0000,0000,0000,,Oh, izvini… Malo sam se zanela.
Dialogue: 0,0:09:34.55,0:09:38.78,Default00,Default,0000,0000,0000,,Ali postoji još jedan razlog zašto tvoja \Noperacija još nije moguća.
Dialogue: 0,0:09:38.99,0:09:42.24,Default00,Default,0000,0000,0000,,Eksplodirajuća Buba u \Ntvom sistemu je premala
Dialogue: 0,0:09:42.28,0:09:44.66,Default00,Default,0000,0000,0000,,da bi je ljudsko oko moglo videti.
Dialogue: 0,0:09:44.76,0:09:47.62,Default00,Default,0000,0000,0000,,Dok ne bude narasla,\Nne možemo ništa uraditi.
Dialogue: 0,0:09:48.07,0:09:51.03,Default00,Default,0000,0000,0000,,Ipak, ti ne možeš samo sedeti i čekati.
Dialogue: 0,0:09:51.65,0:09:54.33,Default00,Default,0000,0000,0000,,Mi ne znamo koliko je čakre potrebno
Dialogue: 0,0:09:54.37,0:09:58.41,Default00,Default,0000,0000,0000,,da Eksplodirajuća Buba poraste\Nu otok i eksplodira!
Dialogue: 0,0:09:58.87,0:10:00.98,Default00,Default,0000,0000,0000,,Zato, daj mi svoju reč, Raikage!
Dialogue: 0,0:10:01.02,0:10:04.24,Default00,Default,0000,0000,0000,,Da ćeš nam dozvoliti da nastavimo\Nsa operacijom na naš način!
Dialogue: 0,0:10:04.24,0:10:07.16,Default00,Default,0000,0000,0000,,Da se nećeš uopšte pomerati.
Dialogue: 0,0:10:07.66,0:10:09.95,Default00,Default,0000,0000,0000,,U redu, dajem ti moju reč!
Dialogue: 0,0:10:21.67,0:10:24.53,Default00,Default,0000,0000,0000,,Izgleda kao da ćeš održati svoju reč.
Dialogue: 0,0:10:25.86,0:10:27.37,Default00,Default,0000,0000,0000,,Šta je bilo?
Dialogue: 0,0:10:29.71,0:10:31.37,Default00,Default,0000,0000,0000,,Samo buba.
Dialogue: 0,0:10:35.19,0:10:36.58,Default00,Default,0000,0000,0000,,Šta ne valja?
Dialogue: 0,0:10:36.87,0:10:38.33,Default00,Default,0000,0000,0000,,Ne, nije ništa.
Dialogue: 0,0:10:38.50,0:10:42.49,Default00,Default,0000,0000,0000,,Svejedno, naša meta je sigurno \Nunutar te napuštene zgrade.
Dialogue: 0,0:10:43.99,0:10:46.78,Default00,Default,0000,0000,0000,,I kako je Raikage?
Dialogue: 0,0:10:47.20,0:10:48.62,Default00,Default,0000,0000,0000,,Nema promene.
Dialogue: 0,0:10:49.89,0:10:53.33,Default00,Default,0000,0000,0000,,Da li si siguran da je udahnuo \NEksplodirajuću Bubu?
Dialogue: 0,0:10:53.43,0:10:54.91,Default00,Default,0000,0000,0000,,Nemam sumnje u vezi toga.
Dialogue: 0,0:10:54.95,0:10:57.37,Default00,Default,0000,0000,0000,,Vreme i udaljenost, sve je bilo precizno.
Dialogue: 0,0:10:57.91,0:10:59.28,Default00,Default,0000,0000,0000,,Onda zašto?
Dialogue: 0,0:11:02.03,0:11:04.87,Default00,Default,0000,0000,0000,,Zašto Eksplodirajuća Buba ne sazreva?
Dialogue: 0,0:11:05.13,0:11:09.24,Default00,Default,0000,0000,0000,,Da to znam, mi ne bi bili ovde \Ni njih špijunirali.
Dialogue: 0,0:11:09.62,0:11:12.12,Default00,Default,0000,0000,0000,,Hajde da ih napadnemo.
Dialogue: 0,0:11:12.74,0:11:15.41,Default00,Default,0000,0000,0000,,Čekajući ovako je dosadno.
Dialogue: 0,0:11:15.52,0:11:16.83,Default00,Default,0000,0000,0000,,Ne budi glupav.
Dialogue: 0,0:11:16.89,0:11:19.78,Default00,Default,0000,0000,0000,,Jedan od njih mogao bi \Neksplodirati bilo kada.
Dialogue: 0,0:11:20.16,0:11:24.33,Default00,Default,0000,0000,0000,,Ako ih napadnemo sad,\Nmogli bi i mi završiti mrtvi.
Dialogue: 0,0:11:25.01,0:11:27.33,Default00,Default,0000,0000,0000,,Ne zaboravi naš cilj.
Dialogue: 0,0:11:27.42,0:11:31.20,Default00,Default,0000,0000,0000,,Mi smo unajmljeni da vratimo \Nsvitak nazad, netaknut.
Dialogue: 0,0:11:31.80,0:11:34.83,Default00,Default,0000,0000,0000,,Najlakši način je da pregovaramo\No razmeni.
Dialogue: 0,0:11:34.87,0:11:38.30,Default00,Default,0000,0000,0000,,Mi uklonimo Eksplodirajuću Bubu\Nu zamenu za svitak.
Dialogue: 0,0:11:38.52,0:11:41.24,Default00,Default,0000,0000,0000,,Nije bitno da li će neprijatelj \Nživeti ili umreti,
Dialogue: 0,0:11:41.28,0:11:43.76,Default00,Default,0000,0000,0000,,ali mi moramo povratiti svitak neoštećen!
Dialogue: 0,0:11:44.45,0:11:47.33,Default00,Default,0000,0000,0000,,Tada ćemo biti potpuno nagrađeni.
Dialogue: 0,0:11:47.54,0:11:51.16,Default00,Default,0000,0000,0000,,Izgleda da su drugi došli da \Npomognu neprijatelju.
Dialogue: 0,0:11:51.33,0:11:53.74,Default00,Default,0000,0000,0000,,Zato kao mera opreza,
Dialogue: 0,0:11:53.78,0:11:56.81,Default00,Default,0000,0000,0000,,mi ćemo ponovo raštrkati još malo \NEksplodirajućih Buba…
Dialogue: 0,0:11:56.85,0:11:59.94,Default00,Default,0000,0000,0000,,kada se svi budu sakupili \Nna jednom mestu.
Dialogue: 0,0:12:06.52,0:12:08.49,Default00,Default,0000,0000,0000,,Doneo sam antiseptik.
Dialogue: 0,0:12:09.20,0:12:10.33,Default00,Default,0000,0000,0000,,Hvala.
Dialogue: 0,0:12:10.41,0:12:14.33,Default00,Default,0000,0000,0000,,Mogu li asistirati? Voleo bih da naučim\Nzbog budućeg iskustva…
Dialogue: 0,0:12:14.53,0:12:16.20,Default00,Default,0000,0000,0000,,Cenila bih to.
Dialogue: 0,0:12:16.62,0:12:19.58,Default00,Default,0000,0000,0000,,Usput, da li znaš da izvodiš\NHemostatičku Veštinu?
Dialogue: 0,0:12:19.67,0:12:21.33,Default00,Default,0000,0000,0000,,Oh da, možeš računati na mene.
Dialogue: 0,0:12:21.82,0:12:23.70,Default00,Default,0000,0000,0000,,To će biti od velike pomoći.
Dialogue: 0,0:12:23.86,0:12:26.20,Default00,Default,0000,0000,0000,,Molim te idi s druge strane i pomozi mi.
Dialogue: 0,0:12:26.42,0:12:28.91,Default00,Default,0000,0000,0000,,Ovo će uštediti mnogo vremena,
Dialogue: 0,0:12:28.95,0:12:32.86,Default00,Default,0000,0000,0000,,umesto da kontrolišem krvarenje\Ni još sama radim na rezovima.
Dialogue: 0,0:12:33.08,0:12:35.28,Default00,Default,0000,0000,0000,,Hvala… Amai.
Dialogue: 0,0:12:36.12,0:12:38.41,Default00,Default,0000,0000,0000,,Ne brini.
Dialogue: 0,0:12:38.44,0:12:41.90,Default00,Default,0000,0000,0000,,Lady Tsunade će izvaditi svaku \Nmalecku bubu iz tebe.
Dialogue: 0,0:12:42.12,0:12:45.91,Default00,Default,0000,0000,0000,,Počećemo anesteziju, \Nzato molim te, opusti se.
Dialogue: 0,0:12:53.86,0:12:57.08,Default00,Default,0000,0000,0000,,Da li si spremna da počneš operaciju?
Dialogue: 0,0:12:57.34,0:13:00.03,Default00,Default,0000,0000,0000,,Da, upravo smo mu dali anesteziju.
Dialogue: 0,0:13:00.41,0:13:03.24,Default00,Default,0000,0000,0000,,Počeće da deluje za minut ili dva.
Dialogue: 0,0:13:03.74,0:13:06.99,Default00,Default,0000,0000,0000,,U redu, Tsunade!\NPobrini se za Karaia!
Dialogue: 0,0:13:08.87,0:13:11.37,Default00,Default,0000,0000,0000,,Shizune, obavi to.
Dialogue: 0,0:13:11.86,0:13:14.62,Default00,Default,0000,0000,0000,,Šta? Zašto ti ne uzimaš skalpel?
Dialogue: 0,0:13:14.68,0:13:16.33,Default00,Default,0000,0000,0000,,Mislio sam da ćeš ti…
Dialogue: 0,0:13:16.37,0:13:19.20,Default00,Default,0000,0000,0000,,Tišina! Naredila sam, da Shizune ovo uradi!
Dialogue: 0,0:13:19.43,0:13:21.24,Default00,Default,0000,0000,0000,,Ne govori mi da ćutim!
Dialogue: 0,0:13:21.35,0:13:22.91,Default00,Default,0000,0000,0000,,Koliko ja znam, činjenica je
Dialogue: 0,0:13:22.95,0:13:26.26,Default00,Default,0000,0000,0000,,da si ti najbolji Medicinski Nindža\Nkoji postoji, zbog toga…
Dialogue: 0,0:13:26.28,0:13:27.95,Default00,Default,0000,0000,0000,,Ne brini oko toga!
Dialogue: 0,0:13:28.01,0:13:30.45,Default00,Default,0000,0000,0000,,Shizune je visoko stručni\NMedicinski Nindža!
Dialogue: 0,0:13:30.49,0:13:32.87,Default00,Default,0000,0000,0000,,Sem toga, dao si mi svoju reč…
Dialogue: 0,0:13:32.90,0:13:36.19,Default00,Default,0000,0000,0000,,Da ćeš nam dozvoliti da radimo\Noperaciju na naš način!
Dialogue: 0,0:13:36.22,0:13:37.73,Default00,Default,0000,0000,0000,,U pravu si, jesam.
Dialogue: 0,0:13:37.83,0:13:41.21,Default00,Default,0000,0000,0000,,Ali ćeš zažaliti ako se bilo šta \Ndogodi Karaiu!
Dialogue: 0,0:13:41.43,0:13:44.03,Default00,Default,0000,0000,0000,,Ne brini, uskočiću ako bude trebalo.
Dialogue: 0,0:13:44.74,0:13:47.62,Default00,Default,0000,0000,0000,,Zašto ne odeš dole i odmoriš malo?
Dialogue: 0,0:13:47.97,0:13:49.58,Default00,Default,0000,0000,0000,,Kako ide, Shizune?
Dialogue: 0,0:13:49.84,0:13:51.62,Default00,Default,0000,0000,0000,,Okej! Spremni smo bilo kada!
Dialogue: 0,0:13:51.74,0:13:53.45,Default00,Default,0000,0000,0000,,Dobro, onda počni.
Dialogue: 0,0:13:54.80,0:13:57.37,Default00,Default,0000,0000,0000,,U redu. Sada ćemo početi proceduru
Dialogue: 0,0:13:57.41,0:14:01.06,Default00,Default,0000,0000,0000,,da otvorimo grudni koš\Ni uklonimo Eksplodirajuću bubu.
Dialogue: 0,0:14:01.08,0:14:03.29,Default00,Default,0000,0000,0000,,Molim te, započni Homostatičku Veštinu.
Dialogue: 0,0:14:03.33,0:14:04.38,Default00,Default,0000,0000,0000,,U redu!
Dialogue: 0,0:14:13.97,0:14:15.20,Default00,Default,0000,0000,0000,,Šta je bilo?
Dialogue: 0,0:14:15.24,0:14:16.46,Default00,Default,0000,0000,0000,,Sve je u redu.
Dialogue: 0,0:14:16.58,0:14:19.71,Default00,Default,0000,0000,0000,,Na sekund, \NEksplodirajuća Buba je porasla.
Dialogue: 0,0:14:19.74,0:14:24.54,Default00,Default,0000,0000,0000,,Moj bože! Kako bi ja trebao da \Nodržim moju reč i ostanem miran?
Dialogue: 0,0:14:24.64,0:14:26.41,Default00,Default,0000,0000,0000,,Šta nije u redu, Tsunade?
Dialogue: 0,0:14:55.54,0:14:57.41,Default00,Default,0000,0000,0000,,Šta nije u redu, Tsunade?
Dialogue: 0,0:15:00.98,0:15:02.03,Default00,Default,0000,0000,0000,,Krv?
Dialogue: 0,0:15:03.37,0:15:04.58,Default00,Default,0000,0000,0000,,Vidim…
Dialogue: 0,0:15:05.83,0:15:09.45,Default00,Default,0000,0000,0000,,Hemofobija…\NDakle, ti se bojiš krvi!
Dialogue: 0,0:15:09.99,0:15:14.62,Default00,Default,0000,0000,0000,,Zato si ti prepustila operaciju\Ntvojoj pomoćnici…
Dialogue: 0,0:15:14.70,0:15:18.55,Default00,Default,0000,0000,0000,,Izvinite me… Izaći ću napolje\Nda dobijem malo vazduha.
Dialogue: 0,0:15:23.42,0:15:28.20,Default00,Default,0000,0000,0000,,Kakvo onesposobljavajuće stanje,\Nda Medicinski Nindža bude s tim proklet…
Dialogue: 0,0:16:05.66,0:16:06.66,Default00,Default,0000,0000,0000,,Navaki…
Dialogue: 0,0:16:13.62,0:16:14.66,Default00,Default,0000,0000,0000,,Dan…
Dialogue: 0,0:16:15.67,0:16:17.28,Default00,Default,0000,0000,0000,,Hej, Shizune…
Dialogue: 0,0:16:17.28,0:16:19.20,Default00,Default,0000,0000,0000,,Reci mi nešto.
Dialogue: 0,0:16:19.63,0:16:21.45,Default00,Default,0000,0000,0000,,Ja sam u sred operacije.
Dialogue: 0,0:16:21.87,0:16:23.08,Default00,Default,0000,0000,0000,,Znam to.
Dialogue: 0,0:16:23.70,0:16:26.28,Default00,Default,0000,0000,0000,,Ne tražim od tebe da staneš.
Dialogue: 0,0:16:26.46,0:16:30.12,Default00,Default,0000,0000,0000,,Tsunade je rame uz rame sa\NJiraiyom i Orochimaruom,
Dialogue: 0,0:16:30.16,0:16:32.93,Default00,Default,0000,0000,0000,,kao jedan od Legendarnih Sanina\Niz sela Skrivenog u Lišću.
Dialogue: 0,0:16:32.95,0:16:36.55,Default00,Default,0000,0000,0000,,Šta se to za ime sveta dogodilo\Nda je učinilo da se boji krvi?
Dialogue: 0,0:16:36.63,0:16:38.33,Default00,Default,0000,0000,0000,,Ja sam isto radoznao…
Dialogue: 0,0:16:38.78,0:16:41.70,Default00,Default,0000,0000,0000,,Iako Lady Tsunade nije iz našeg sela,
Dialogue: 0,0:16:41.74,0:16:45.32,Default00,Default,0000,0000,0000,,ona je oličenje onoga što sve\NMedicinske Nindže žele da dostignu.
Dialogue: 0,0:16:45.38,0:16:47.66,Default00,Default,0000,0000,0000,,Ali ono što sam primetio ranije...
Dialogue: 0,0:16:50.20,0:16:51.28,Default00,Default,0000,0000,0000,,Razumem.
Dialogue: 0,0:16:52.15,0:16:56.06,Default00,Default,0000,0000,0000,,Lady Tsunade je imala dve \Nvelike ljubavi u svom životu.
Dialogue: 0,0:16:57.12,0:16:59.28,Default00,Default,0000,0000,0000,,Prva je bila njen mlađi brat, Navaki.
Dialogue: 0,0:17:00.19,0:17:02.95,Default00,Default,0000,0000,0000,,Ali na dan posle njegovog rođendana,
Dialogue: 0,0:17:02.99,0:17:05.45,Default00,Default,0000,0000,0000,,nakon što mu je ona predala poklon…
Dialogue: 0,0:17:05.49,0:17:09.49,Default00,Default,0000,0000,0000,,on je bio ubijen u borbi za vreme\NTrećeg Velikog Nindža Rata.
Dialogue: 0,0:17:10.06,0:17:12.39,Default00,Default,0000,0000,0000,,Druga osoba bio je čovek kome je
Dialogue: 0,0:17:12.42,0:17:14.83,Default00,Default,0000,0000,0000,,obećala da će s njim deliti svoj život…
Dialogue: 0,0:17:14.87,0:17:17.08,Default00,Default,0000,0000,0000,,Dan Kato, moj ujak.
Dialogue: 0,0:17:18.20,0:17:21.72,Default00,Default,0000,0000,0000,,{\i1}Ja… ne želim još da umrem.{\i0}
Dialogue: 0,0:17:22.37,0:17:27.83,Default00,Default,0000,0000,0000,,{\i1}Ima toliko stvari koje su ostale da uradim…{\i0}
Dialogue: 0,0:17:28.00,0:17:29.74,Default00,Default,0000,0000,0000,,{\i1}Nemoj govoriti!{\i0}
Dialogue: 0,0:17:31.33,0:17:36.28,Default00,Default,0000,0000,0000,,{\i1}Ja… ne mogu umreti… ovde…{\i0}
Dialogue: 0,0:17:36.40,0:17:39.49,Default00,Default,0000,0000,0000,,{\i1}Ne brini,{\i0}\N{\i1}zaustavila sam krvarenje.{\i0}
Dialogue: 0,0:17:39.63,0:17:41.16,Default00,Default,0000,0000,0000,,{\i1}Ti ćeš biti dobro.{\i0}
Dialogue: 0,0:17:41.45,0:17:43.24,Default00,Default,0000,0000,0000,,{\i1}Bićeš dobro, Dan!{\i0}
Dialogue: 0,0:17:44.37,0:17:46.87,Default00,Default,0000,0000,0000,,{\i1}Bože… Tako mi je drago…{\i0}
Dialogue: 0,0:17:56.41,0:17:58.83,Default00,Default,0000,0000,0000,,{\i1}Dođavola… Nemoj iskrvariti!{\i0}
Dialogue: 0,0:17:59.06,0:18:00.20,Default00,Default,0000,0000,0000,,{\i1}Stani!{\i0}
Dialogue: 0,0:18:01.24,0:18:03.62,Default00,Default,0000,0000,0000,,{\i1}Stani! Stani!{\i0}
Dialogue: 0,0:18:04.03,0:18:05.03,Default00,Default,0000,0000,0000,,{\i1}Stani!{\i0}
Dialogue: 0,0:18:05.20,0:18:06.49,Default00,Default,0000,0000,0000,,{\i1}Rekla sam, stani!{\i0}
Dialogue: 0,0:18:21.41,0:18:25.62,Default00,Default,0000,0000,0000,,Od tog dana, Lady Tsunade\Nje traumatizovana.
Dialogue: 0,0:18:27.08,0:18:29.86,Default00,Default,0000,0000,0000,,Ja, isto, mora da sam bio u šoku
Dialogue: 0,0:18:29.88,0:18:32.78,Default00,Default,0000,0000,0000,,nakon što sam video kako Nigai umire\Nispred mojih očiju.
Dialogue: 0,0:18:32.80,0:18:35.24,Default00,Default,0000,0000,0000,,Inače, nikad ne bih kockao naše živote
Dialogue: 0,0:18:35.28,0:18:38.03,Default00,Default,0000,0000,0000,,u rukama Medicinskog Nindže\Nkoji je izgubljen.
Dialogue: 0,0:18:38.08,0:18:40.20,Default00,Default,0000,0000,0000,,Lady Tsunade nije izgubljena!
Dialogue: 0,0:18:44.15,0:18:46.53,Default00,Default,0000,0000,0000,,Uklanjanje Eksplodirajuće Bube je završeno.
Dialogue: 0,0:18:46.58,0:18:47.90,Default00,Default,0000,0000,0000,,Operacija je završena!
Dialogue: 0,0:18:48.95,0:18:50.74,Default00,Default,0000,0000,0000,,Molim te, zatvori ga!
Dialogue: 0,0:18:51.33,0:18:52.16,Default00,Default,0000,0000,0000,,Da!
Dialogue: 0,0:18:52.21,0:18:53.56,Default00,Default,0000,0000,0000,,Još jedna stvar…
Dialogue: 0,0:18:53.60,0:18:56.77,Default00,Default,0000,0000,0000,,Medicinski Ninjutsu Lady Tsunade \Nnije izgubljen.
Dialogue: 0,0:18:56.85,0:18:58.78,Default00,Default,0000,0000,0000,,Ja ću nastaviti umesto nje.
Dialogue: 0,0:18:58.90,0:19:02.33,Default00,Default,0000,0000,0000,,I Lady Tsunade će stati na svoje noge.\NOna mora to!
Dialogue: 0,0:19:03.50,0:19:05.83,Default00,Default,0000,0000,0000,,U ime mog pokojnog ujaka isto!
Dialogue: 0,0:19:10.84,0:19:13.37,Default00,Default,0000,0000,0000,,Posle svega, dobio sam opkladu.
Dialogue: 0,0:19:13.42,0:19:14.60,Default00,Default,0000,0000,0000,,Huh?
Dialogue: 0,0:19:14.87,0:19:17.87,Default00,Default,0000,0000,0000,,Pojačanje koje je Arai pozvao,\Nubrzo je došlo
Dialogue: 0,0:19:17.91,0:19:20.33,Default00,Default,0000,0000,0000,,i Nokizaru Trupe su izgleda odustale.
Dialogue: 0,0:19:21.19,0:19:23.33,Default00,Default,0000,0000,0000,,To je bila samo sreća.
Dialogue: 0,0:19:23.42,0:19:24.20,Default00,Default,0000,0000,0000,,Hmm?
Dialogue: 0,0:19:24.20,0:19:25.74,Default00,Default,0000,0000,0000,,Pa onda, koja je veza
Dialogue: 0,0:19:25.78,0:19:27.49,Default00,Default,0000,0000,0000,,između prevazilaženja tvoje hemofobije
Dialogue: 0,0:19:27.53,0:19:30.99,Default00,Default,0000,0000,0000,,i Narutove pobede u klađenju sa tobom?
Dialogue: 0,0:19:32.16,0:19:33.16,Default00,Default,0000,0000,0000,,To je…
Dialogue: 0,0:19:33.77,0:19:37.66,Default00,Default,0000,0000,0000,,{\i1}Nemoj umreti… Nemoj umreti…{\i0}\N{\i1}Nemoj umreti…{\i0}
Dialogue: 0,0:19:37.78,0:19:38.91,Default00,Default,0000,0000,0000,,{\i1}Nemoj umreti!{\i0}
Dialogue: 0,0:19:39.06,0:19:39.99,Default00,Default,0000,0000,0000,,{\i1}Nemoj umreti!{\i0}
Dialogue: 0,0:19:41.27,0:19:43.78,Default00,Default,0000,0000,0000,,Narutova žilava životna energija i…
Dialogue: 0,0:19:47.62,0:19:49.03,Default00,Default,0000,0000,0000,,{\i1}Postati Hokage…{\i0}
Dialogue: 0,0:19:49.06,0:19:51.89,Default00,Default,0000,0000,0000,,{\i1}– Postati Hokage je moj san.{\i0}\N{\i1}– Postati Hokage je moj san.{\i0}
Dialogue: 0,0:19:52.23,0:19:55.33,Default00,Default,0000,0000,0000,,Ta strastvena volja da postane Hokage…
Dialogue: 0,0:19:59.00,0:20:01.41,Default00,Default,0000,0000,0000,,Još jednom… Samo još jedan zadnji put…
Dialogue: 0,0:20:02.77,0:20:05.37,Default00,Default,0000,0000,0000,,Osećam da želim da se kladim na Naruta.
Dialogue: 0,0:20:10.29,0:20:14.62,Default00,Default,0000,0000,0000,,Da li mi govoriš da sam se složio sa \Nsmešnom opkladom bez šansi?
Dialogue: 0,0:20:15.22,0:20:16.91,Default00,Default,0000,0000,0000,,Prekasno je da se žališ.
Dialogue: 0,0:20:17.16,0:20:19.24,Default00,Default,0000,0000,0000,,Prihvati to i pomiri se s tim.
Dialogue: 0,0:20:19.47,0:20:20.95,Default00,Default,0000,0000,0000,,Ne budi smešna!
Dialogue: 0,0:20:21.07,0:20:22.64,Default00,Default,0000,0000,0000,,Bio sam spreman…
Dialogue: 0,0:20:22.70,0:20:25.81,Default00,Default,0000,0000,0000,,Još od kada su \NSavezničke Nindža Snage formirane.
Dialogue: 0,0:20:25.98,0:20:27.37,Default00,Default,0000,0000,0000,,Oh, to je istina.
Dialogue: 0,0:20:28.33,0:20:31.74,Default00,Default,0000,0000,0000,,Ogrlica Prvog Hokagea.\NSada hoću to da vidim.
Dialogue: 0,0:20:31.78,0:20:33.11,Default00,Default,0000,0000,0000,,Dođi i pokaži mi je.
Dialogue: 0,0:20:33.24,0:20:34.61,Default00,Default,0000,0000,0000,,Nemam je više.
Dialogue: 0,0:20:41.38,0:20:44.24,Default00,Default,0000,0000,0000,,Mislim da kada je\NDevetorepa bila izašla…
Dialogue: 0,0:20:44.28,0:20:46.37,Default00,Default,0000,0000,0000,,U svemu tome ja sam je uništio.
Dialogue: 0,0:20:46.41,0:20:49.17,Default00,Default,0000,0000,0000,,Hej, hej…\NZar to nije bilo nešto važno?
Dialogue: 0,0:20:49.28,0:20:50.28,Default00,Default,0000,0000,0000,,U redu je.
Dialogue: 0,0:20:50.62,0:20:51.70,Default00,Default,0000,0000,0000,,Baka i ja…
Dialogue: 0,0:20:51.74,0:20:55.91,Default00,Default,0000,0000,0000,,mi držimo to obećanje duboko\Nu našim srcima.
Dialogue: 0,0:20:57.33,0:20:58.16,Default00,Default,0000,0000,0000,,Jo!
Dialogue: 0,0:20:58.24,0:20:59.31,Default00,Default,0000,0000,0000,,Požurimo!
Dialogue: 0,0:21:00.20,0:21:02.28,Default00,Default,0000,0000,0000,,Kladim se da svi čekaju!
Dialogue: 0,0:22:37.10,0:22:38.95,Default00,Default,0000,0000,0000,,Ti uživaš u ubijanju.
Dialogue: 0,0:22:39.03,0:22:41.89,Default00,Default,0000,0000,0000,,Mrzim da pomislim da nas oboje \Nnazivaju nindžama…
Dialogue: 0,0:22:42.03,0:22:43.37,Default00,Default,0000,0000,0000,,Kako arogantno…
Dialogue: 0,0:22:43.42,0:22:46.37,Default00,Default,0000,0000,0000,,Ko ti je dao dozvolu da koristiš\NKrvnikov Mač?
Dialogue: 0,0:22:46.41,0:22:49.05,Default00,Default,0000,0000,0000,,Taj mač pripada Sedmorici\NNindža Mačevalaca iz Magle!
Dialogue: 0,0:22:49.08,0:22:51.21,Default00,Default,0000,0000,0000,,Stranac koji ga koristi,\Nto je neprihvatljivo!
Dialogue: 0,0:22:51.26,0:22:52.54,Default00,Default,0000,0000,0000,,Ne sviđa mi se ni malo.
Dialogue: 0,0:22:52.58,0:22:55.92,Default00,Default,0000,0000,0000,,Sam da se borim s ovo dvoje,\Nto će biti teško, ali
Dialogue: 0,0:22:56.91,0:23:01.41,Default00,Default,0000,0000,0000,,Sledeća epizoda:\N"OPASNOST: JINPACHI I KUSHIMARU!"
Dialogue: 0,0:23:01.52,0:23:04.16,Default00,Default,0000,0000,0000,,Izgleda kao da si sateran u ćošak, drugar!\N\N\N"OPASNOST: JINPACHI I KUSHIMARU!"
Dialogue: 0,0:23:04.20,0:23:05.03,Default00,Default,0000,0000,0000,,"OPASNOST: JINPACHI I KUSHIMARU!"
Dialogue: 0,0:23:05.08,0:23:09.20,Dodatak,Default,0000,0000,0000,,Prevod i obrada:\NANUBISBUCA
Dialogue: 0,0:23:09.20,0:23:11.20,Dodatak,,0000,0000,0000,,


